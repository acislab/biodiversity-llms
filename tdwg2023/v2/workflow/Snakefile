ABSENCE_OUT = f"results/{LLM}/absence.tsv"

include: "rules/make-presence-tsv.smk"
include: "rules/make-absence-tsv.smk"
include: "rules/qa.smk"

rule all:
    input:
        PRESENCE_OUT,
        ABSENCE_OUT
